	sky81294:skywork,sky81294{
		cell-index = <0>;
		compatible = "skywork,sky81294";
		status = "okay";
		qcom,cci-master = <0>;
		qcom,slave-id = <0x6e 0x00 0x0f>;
		
		label = "sky81294";
		qcom,gpio-no-mux = <0>;
		qcom,enable_pinctrl;
		pinctrl-names = "cam_flash_default", "cam_flash_suspend";
		pinctrl-0 = <&cam_sensor_flash_default>;
		pinctrl-1 = <&cam_sensor_flash_sleep>;
		gpios = <&msm_gpio 31 0>,<&msm_gpio 32 0>,<&msm_gpio 27 0>;
		qcom,gpio-flash-en = <0>;
		qcom,gpio-flash-now = <1>;
		qcom,gpio-vio = <2>;
		
		qcom,gpio-req-tbl-num = <0 1 2>;
		qcom,gpio-req-tbl-flags = <0 0 0>;
		qcom,gpio-req-tbl-label = "FLASH_ENABLE", "FLASH_INHIBIT", "Gpio-vio";
		
		qcom,current = <200>;
		qcom,max-current = <1000>;
	};

	lm3642:ti,lm3642{
		cell-index = <1>;
		compatible = "ti,lm3642";
		status = "okay";
		qcom,cci-master = <0>;
		qcom,slave-id = <0xc6 0x00 0x00>;
		label = "lm3642";
		qcom,gpio-no-mux = <0>;
		qcom,enable_pinctrl;
		pinctrl-names = "cam_flash_default", "cam_flash_suspend";
		pinctrl-0 = <&cam_sensor_flash_default>;
		pinctrl-1 = <&cam_sensor_flash_sleep>;
		gpios = <&msm_gpio 32 0>,<&msm_gpio 31 0>,<&msm_gpio 27 0>;
		qcom,gpio-flash-en = <0>;
		qcom,gpio-flash-now = <1>;
		qcom,gpio-vio = <2>;
		
		qcom,gpio-req-tbl-num = <0 1 2>;
		qcom,gpio-req-tbl-flags = <0 0 0>;
		qcom,gpio-req-tbl-label = "FLASH_EN", "FLASH_NOW", "Gpio-vio";
		
		qcom,current = <200>;
		qcom,max-current = <950>;
	};
